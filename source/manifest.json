{
	"manifest_version": 2,
	"name": "Superlinks",
	"version": "1.0",
	"description": "Replaces the hyperlinks with blue underlined text.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
    "browser_action": {
        "default_icon": "icon.png"
    }
}
